    "@nuxt/schema": "^3.7.3",
    "@nuxt/test-utils": "^3.7.3",
    "@types/node": "^18.17.17",
    "changelogen": "^0.5.5",
    "dayjs": "^1.11.10",
    "eslint": "^8.49.0",
    "nuxt": "^3.7.3",
    "prettier": "^3.0.3",
    "vitest": "^0.33.0"
