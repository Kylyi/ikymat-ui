    "@unocss/nuxt": "^0.56.2",
    "@vueuse/core": "^10.4.1",
    "@vueuse/motion": "^2.0.0",
    "@vueuse/nuxt": "^10.4.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
